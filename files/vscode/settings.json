			},
			{
					"name": "pdflatex ðŸ”ƒ",
					"tools": [
							"pdflatex"
					]
			},
			{
					"name": "pdflatex âžž bibtex âžž pdflatexâ€ŠÃ—â€Š2",
					"tools": [
							"pdflatex",
							"bibtex",
							"pdflatex",
							"pdflatex"
